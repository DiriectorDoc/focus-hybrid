	"version": "0.0.2",
	"publisher": "DiriectorDoc",
	"author": {
		"name": "Diriector_Doc"
	},
	"license": "MIT",
	"engines": {
		"vscode": "^1.54.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/DiriectorDoc/focus-hybrid"
	},
	"bugs": {
		"url": "https://github.com/DiriectorDoc/focus-hybrid/issues"
